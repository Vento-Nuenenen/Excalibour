{
    "name": "Chronyms/Excalibour",
    "description": "An Exer-Management App",
    "keywords": ["Project", "Excalibour", "Exer", "Scout"],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "appstract/laravel-opcache": "^2.0",
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "creativeorange/gravatar": "~1.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~4.0",
        "intervention/image": "^2.4",
        "jaybizzle/laravel-crawler-detect": "1.*",
        "jeremykenedy/laravel-exception-notifier": "^1.1",
        "jeremykenedy/laravel-https": "^1.1",
        "jeremykenedy/laravel-logger": "^2.0.0",
        "jeremykenedy/laravel-roles": "^1.1",
        "jeremykenedy/uuid": "^3.1",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "pusher/pusher-php-server": "^3.0",
        "rap2hpoutre/laravel-log-viewer": "^0.18.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.7",
        "mockery/mockery": "1.1.0",
        "phpunit/phpunit": "~7.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
